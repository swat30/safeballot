* {
	margin: 0;
	padding: 0;
}

table {
	margin: 0;
	padding: 0;
}

div.filebrowsericon {
	float: left; 
	padding: 8px;
	width: 60px;
	height: 80px;
	text-align: center;
	cursor: pointer;
}

div.filebrowsericon:hover {
	background-color: #E0E0E0;
}

div.fileinfo {
	display: none;
}

.fileOps {
	width: 125px;
	border-right: 1px solid #000;
	padding: 0 5px 0 0;
	margin: 0 5px 0 0 ;
}

#fileInfoShow {
	padding: 5px 0 0 0;
	font-size: 0.9em;
}