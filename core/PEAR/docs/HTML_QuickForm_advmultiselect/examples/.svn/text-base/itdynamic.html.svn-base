{qf_javascript}
{ams_javascript}
</head>
<body>

<table width="100%" border="0">
<tr>
  <td colspan="3" style="background-color:#FF9900"><h1>HeadBanner</h1></td>
</tr>
<tr>
  <td style="background-color:red; vertical-align:top; width:15%; text-align:left">
    <span style="font-color:white">pseudo-menu 1</span>&nbsp;
  </td>
  <td style="background-color:red; vertical-align:top; width:15%; text-align:left">
    <span style="font-color:white">pseudo-menu 2</span>&nbsp;
  </td>
  <td style="background-color:#FF9900">
    &nbsp;
  </td>
</tr>
<tr>
  <td colspan="3" style="background-color:#EEEEEE; vertical-align:top;">

    <form {qf_attributes}>
    <table class="maintable">

    <!-- BEGIN qf_hidden_block -->
    <div style="display: none;">
      <!-- BEGIN qf_hidden_loop -->{qf_hidden}<!-- END qf_hidden_loop -->
    </div>
    <!-- END qf_hidden_block -->

    <!-- BEGIN qf_main_loop -->
      <!-- BEGIN qf_header -->
      <tr><th class="maintable" colspan="2">{qf_header}</th></tr>
      <!-- END qf_header -->

      <!-- BEGIN qf_element -->
      <tr>
        <td style="text-align:right; width:30%"><!-- BEGIN qf_element_required --><span style="color: #FF0000;">*</span>&nbsp;<!-- END qf_element_required --><span class="qfLabel">{qf_label}&nbsp;</span></td>
        <td>{qf_element}<!-- BEGIN qf_element_error --><br /><span style="color: #FF0000;">{qf_error}</span><!-- END qf_element_error --></td>
      </tr>
      <!-- END qf_element -->

    <!-- END qf_main_loop -->
    </table>
    </form>

</td>
</tr>
<tr>
  <td colspan="3" style="background-color:white; text-align:right">
    <span style="color:black; font-weight:bold">powered by PEAR</span>&nbsp;
  </td>
</tr>
</table>

{message_form_validate}

</body>
</html>