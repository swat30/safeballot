      <div class="refnamediv">
        <h2>
          Name
        </h2>
        <p>
          <code class=
          "function">HTML_QuickForm_advmultiselect::setElementTemplate</code> -
          Sets element template
        </p>
      </div>
      <div class="refsynopsisdiv">
        <h2>
          Synopsis
        </h2>
        <div class="funcsynopsis">
          <pre class="funcsynopsisinfo">
      require_once 'HTML/QuickForm/advmultiselect.php';
     
</pre>
          <table border="0" summary="Function synopsis" cellspacing="0"
          cellpadding="0" class="c1">
            <tr>
              <td>
                <code class="funcdef">void <b class=
                "fsfunc">HTML_QuickForm_advmultiselect::setElementTemplate</b>(</code>
              </td>
              <td>
                <var class="pdparam">$html</var><code>)</code>;
              </td>
              <td>
                 
              </td>
            </tr>
          </table>
          <table border="0" summary="Function argument synopsis" cellspacing=
          "0" cellpadding="0">
            <tr>
              <td>
                string  
              </td>
